<!DOCTYPE html>

<title>Ticket</title>

{% block body %}
    <h1>Ticket</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ticket.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ ticket.code }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ ticket.prix }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ ticket.dateDebut ? ticket.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ ticket.dateFin ? ticket.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_ticket_front_index') }}">back to list</a>

    <a href="{{ path('app_ticket_front_edit', {'id': ticket.id}) }}">edit</a>

    {{ include('ticket_front/_delete_form.html.twig') }}
{% endblock %}
